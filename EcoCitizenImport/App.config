<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
	</configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
		<targets>
			<target name="file" xsi:type="File" fileName="${basedir}/EcoCitizenImport.log" layout="${longdate}|${level}|T${threadid}|${message}|${onexception:${exception:format=stacktrace:maxInnerExceptionLevel=10:innerFormat=message}}" archiveAboveSize="10000000" maxArchiveFiles="100" />
			<target name="console" xsi:type="Console" layout="${longdate}|${level}|T${threadid}|${message}|${onexception:${exception:format=stacktrace:maxInnerExceptionLevel=10:innerFormat=message}}" />
		</targets>
		<rules>
			<logger name="*" minlevel="Trace" writeTo="file" />
			<logger name="*" minlevel="Info" writeTo="console" />
		</rules>
	</nlog>
  <runtime>
